generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model alchemist {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  dialog   String
  info     String
  no       Int
  question String
  yes      Int
}

